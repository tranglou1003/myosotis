# Docker ignore file for VoiceClone Studio
# Ignore development files
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore build outputs
frontend/build/
frontend/dist/
__pycache__/
*.pyc
*.pyo
*.pyd
.Python

# Ignore logs
logs/
*.log

# Ignore environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore git
.git/
.gitignore

# Ignore IDE files
.vscode/
.idea/
*.swp
*.swo

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore temporary files
tmp/
temp/
*.tmp
*.temp

# Ignore uploaded files and results (keep structure but not content)
uploads/*
!uploads/.gitkeep
results/*
!results/.gitkeep

# Ignore model checkpoints (too large for container)
model_english/*.pt
model_english/*.safetensors
f5_tts_repo/ckpts/

# Ignore documentation
docs/
*.md
README*

# Ignore test files
test/
tests/
*.test.js
*.spec.js

# Ignore docker files in build context
Dockerfile*
docker-compose*
.dockerignore
